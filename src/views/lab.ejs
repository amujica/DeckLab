      <!--OPERATIONS--------------------->
      <div  class="operations p-3 mb-2">

            
       
        

          <!--INVERTIR---->        
          <button class="btn btn-outline-dark ml-1 "  id="invertir" disabled> Inverse</button>

          <!--CORTAR---->  
          <button class="btn btn-outline-dark ml-1"  id="cut" disabled> Cut</button>

          <!--FARO SHUFFLE EXT-->
          <button class="btn btn-outline-dark  ml-1 " id="faroExt"> Out Faro</button>

          <!--FARO SHUFFLE INT-->
          <button class="btn btn-outline-dark  ml-1 " id="faroInt"> Int Faro</button>    
          
          <!--FARO SHUFFLE EXT PARCIAL-->
          <button class="btn btn-outline-dark  ml-1 " id="faroExtPar" disabled> Partial Out Faro</button>

          <!--FARO SHUFFLE INT PARCIAL-->
          <button class="btn btn-outline-dark  ml-1 " id="faroIntPar" disabled> Partial Int Faro</button>
          
          <!--ANTIFARO EXT-->
          <button class="btn btn-outline-dark  ml-1" id="antifaroExt" > Out Antifaro</button>

          <!--ANTIFARO INT-->
          <button class="btn btn-outline-dark  ml-1" id="antifaroInt" > Int Antifaro</button>
          
           <!--CHANGE CARD POSITION-->
           <button class="btn btn-outline-dark ml-1  " id="change" disabled> Cull</button>
          <!--CLEAR-->
          <button class="btn btn-outline-dark  ml-1" id="clearLast" disabled> Clear last operation</button>
        
