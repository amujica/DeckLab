        <!--ORIGIN STACK-->
      
        <div class="origin mb-1 ml-5 mr-5" >
          <div>
            <label class="mr-2" for="stacks">Select origin stack </label>
            <select class = "mr-2" id="stacks" class="mb-2">
              <option value="Mnemonica">Mnemonica</option>
              <option value="Aronson">Aronson</option>
              <option value="New Deck Order">New Deck Order</option>
              <option value="New Deck Order 1">New Deck Order (1)</option>
              <option value="New Deck Order 2">New Deck Order (2)</option>
              <option value="Eight Kings">Eight Kings</option>
              <option value="Randomly Shuffled">Randomly Shuffled</option>
              <option value="Si Stebbins">Si Stebbins</option>
            </select>
            <button  id="apply_stack"> Apply</button>
          </div>

          <button class="btn btn-link ml-5"  id="downloadStack"
            title="Download .txt and jpeg files with this stack" 
            onclick="downloadStack()">Download Stack</button>
  
        </div>
      
